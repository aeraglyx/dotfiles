    "custom/os": {
        "format": "Ôåì",
        "tooltip-format": "nixos, btw"
    },
    "hyprland/workspaces": {
        "format": "{name}",
        "on-click": "activate",
        "on-scroll-up": "hyprctl dispatch workspace e-1",
        "on-scroll-down": "hyprctl dispatch workspace e+1",
        // "ignore-workspaces": ["7", "9"]
    },
    "hyprland/window": {
        "format": "{class} | {title}",
        "rewrite": {
            "(.*) ‚Äî Mozilla Firefox": "$1",
            "(.*)Mozilla Firefox.*": "$1New Tab",
            "(.*) ‚Äî Tor Browser": "$1",
            "(.*) - Blender .*": "$1",
        },
        "max-length": 64,
        "separate-outputs": true
    },
    "custom/song": {
        "format": "ÔÜº  {}",
        "tooltip": false,
        "exec": "python ~/.config/waybar/scripts/song.py",
        "interval": 1,
        "on-click": "hyprctl dispatch workspace 7"
    },
    "custom/nightlight": {
        "format": "ÔÉ´ {}",
        // "tooltip": false,
        "exec": "python ~/.config/waybar/scripts/night_light.py",
        "return-type": "json",
        "interval": 300
        // "hide-empty-text": true,
        // "on-click": ""
    },
    "custom/vpn": {
        "format": "Ôè≠ {}",
        "exec": "echo VPN",
        "exec-if": "[[ $(mullvad status | head -n 1) != Connected ]]",
        "interval": 5,
        "on-click": "mullvad-vpn"
    },
    "custom/time-tracker": {
        "format": "{}",
        "exec": "cat ~/.local/share/time/status | sed 's/.*/\\U&/'",
        "interval": 2,
    },
    "custom/screen-recorder": {
        "format": "ÔÑë REC",
        "exec": "pgrep gpu-screen-reco",
        "interval": 2,
        "on-click": "pkill -SIGINT gpu-screen-reco"
    },
    "clock": {
        "format": "{:%H:%M}",
        "tooltip-format": "{:%A %Y-%m-%d}",
    },
    "group/hardware": {
        "orientation": "horizontal",
        "modules": [
            "cpu",
            "custom/gpu",
            "memory",
            "disk"
        ]
    },
    "cpu": {
        "format": "CPU {usage}%",
        "interval": 5,
        "states": {
            "critical": 80,
        },
    },
    "custom/gpu": {
        "format": "GPU {text}%",
        "interval": 5,
        "exec": "nvidia-smi --query-gpu=utilization.gpu --format=csv,noheader,nounits",
        "states": {
            // TODO: why no worky
            "critical": 80,
        },
    },
    "memory": {
        "format": "RAM {percentage}%",
        "interval": 5,
        "states": {
            "critical": 80,
        },
    },
    "disk": {
        "format": "ÔóΩ {percentage_used}%",
        "tooltip-format": "{used} / {total}",
        "interval": 30,
        "path": "/",
        "states": {
            "critical": 80,
        },
    },
    "network": {
        "format": "",
        "format-disconnected": "touch some grass bro",
        "tooltip": false
    },
    "pulseaudio": {
        // FIXME: works every other scroll
        "scroll-step": 2.5, // %, can be a float
        // "format": "{volume}% {icon} {format_source}",
        "format": "{icon} {volume}%",
        "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
        "format-bluetooth-muted": "Ôö© {icon}Ôäî {format_source}",
        "format-muted": "Ôö© {format_source}",
        "format-source": "{volume}% ÔÑ∞",
        "format-source-muted": "ÔÑ±",
        "format-icons": {
            "headphone": "Ôñè",
            // "headphone": "ÔÄ•",
            "hands-free": "Ôñê",
            "headset": "Ôñê",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        },
        "states": {
            // "warning-high": 50,
            "normal": 45,
            "critical": 5
        },
        "on-click": "pavucontrol"
    },
