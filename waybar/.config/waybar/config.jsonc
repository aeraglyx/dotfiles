    "spacing": 14,
    "margin": "14 14 0 14",
    "modules-left": [
        "custom/os",
        "hyprland/workspaces",
        "custom/song"
        // "wlr/taskbar",
        // "hyprland/window"
    ],
    "modules-center": [
        "clock"
    ],
    "modules-right": [
        "custom/screen-recorder",
        "custom/time-tracker",
        "custom/vpn",
        "network",
        "pulseaudio",
        "group/hardware"
        // "custom/notification"
